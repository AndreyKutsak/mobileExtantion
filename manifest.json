{
	"name": "baza Mobile",
	"short_name": "baza",
	"version": "3.1.0",
	"description": "mobile version baza",
	"permissions": [
		"activeTab",
		"tabs",
		"alarms",
		"notifications"
	],
	"background": {
		"service_worker": "background.js",
		"persistance": true
	},
	"web_accessible_resources": [
		{
			"resources": [
				"img/*"
			],
			"matches": [
				"https://baza.m-p.in.ua/*"
			]
		}
	],
	"content_scripts": [
		{
			"matches": [
				"https://baza.m-p.in.ua/*"
			],
			"js": [
				"content-script.js",
				"html5-qrcode.min.js"
			],
			"css": [
				"css/mobile.css"
			],
			"type": "module"
		}
	],
	"manifest_version": 3
}